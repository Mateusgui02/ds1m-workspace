package br.senai.sp.jandira;

import br.senai.sp.jandira.model.Conta;

public class App {

	public static void main(String[] args) {
	
		Conta contaJoãoVitor;
		Conta contaPedro;
		
		contaJoãoVitor = new Conta();
		contaPedro = new Conta();
		
		//*** Conta do João
		contaJoãoVitor.numero = "111-0";
		contaJoãoVitor.titular = "João Vitor Araujo";
		contaJoãoVitor.tipo ="Corente";
		contaJoãoVitor.saldo = 2000.0;
		contaJoãoVitor.ativa = true;
		
		//*** Conta Pedro
		contaPedro.numero = "222-0";
		contaPedro.numero = "222-0";
		contaPedro.tipo ="Poupança";
		contaPedro.saldo = 1500;
		contaPedro.ativa =true;
		
		System.out.println("Nome: " + contaJoãoVitor.titular);
		System.out.println("Saldo: " + contaJoãoVitor.saldo);
		
		System.out.println("Nome: " + contaPedro.titular);
		System.out.println("Saldo: " + contaPedro.saldo);
	}

}
